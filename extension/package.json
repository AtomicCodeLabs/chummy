{
  "name": "chummy",
  "version": "1.0.0",
  "description": "An IDE workflow for Github",
  "extensionId": "ocmdenamdoeigigibgjfnconlhpekfgb",
  "main": "index.js",
  "author": "Alex Kim",
  "email": "chummy@atomiccode.io",
  "license": "AGPL-3.0-only",
  "scripts": {
    "creds": "aws s3 cp s3://chummy-credentials/.env.development .env.development && aws s3 cp s3://chummy-credentials/key.pem key.pem",
    "dev:web": "cross-env NODE_ENV=development webpack --mode=development --watch --config ./webpack/dev.web.config.js",
    "dev:moz": "cross-env NODE_ENV=development webpack --mode=development --watch --config ./webpack/dev.moz.config.js",
    "free:commit": "HUSKY_SKIP_HOOKS=1 git commit -m",
    "mock": "amplify mock",
    "format:check": "prettier --check \"**/*.+(js|jsx|json|css|md)\"",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "lint:check": "eslint --ext .js,.jsx .",
    "lint": "eslint --fix --ext .js,.jsx .",
    "serve": "serve dist",
    "build:dev:web": "cross-env NODE_ENV=production webpack --mode=production --config ./webpack/dev.config.js",
    "build:dev:moz": "cross-env NODE_ENV=production webpack --mode=production --config ./webpack/moz.config.js",
    "build:web": "cross-env NODE_ENV=production webpack --mode=production --config ./webpack/prod.web.config.js",
    "build:moz": "cross-env NODE_ENV=production webpack --mode=production --config ./webpack/prod.moz.config.js",
    "release": "HUSKY_SKIP_HOOKS=1 yarn standard-version -- --no-verify",
    "release:dry": "yarn standard-version -- --dry-run",
    "prerelease": "yarn release -- --prerelease alpha",
    "clean": "rm -rf dist",
    "cy:open": "yarn concurrently \"yarn dev\" \"yarn serve\" \"cross-env TARGET=dev cypress open --env TARGET=dev --config baseUrl=http://localhost:5000/dev\"",
    "cy:open:web": "yarn concurrently \"cross-env TARGET=web cypress open --env TARGET=web --config baseUrl=http://localhost:5000/web\" \"yarn serve\"",
    "cy:run:moz": "yarn concurrently --kill-others --success first \"cross-env TARGET=moz cypress run --env TARGET=moz --config baseUrl=http://localhost:5000/moz,video=false --browser firefox --headless\" \"yarn serve\"",
    "cy:run:web": "yarn concurrently --kill-others --success first \"cross-env TARGET=web cypress run --env TARGET=web --config baseUrl=http://localhost:5000/web,video=false --headless\" \"yarn serve\"",
    "checkout:dev": "git checkout extension/dev && git pull && amplify env checkout dev && amplify pull --yes",
    "checkout:gamma": "git checkout extension/gamma && git pull && amplify env checkout gamma && amplify pull --yes",
    "checkout:prod": "git checkout extension/prod && git pull && amplify env checkout prod && amplify pull --yes",
    "promote:to-dev": "./scripts/promote.dev.sh",
    "promote:to-gamma": "./scripts/promote.gamma.sh",
    "promote:to-prod": "./scripts/promote.prod.sh",
    "promote:all": "yarn promote:to-dev && yarn promote:to-gamma && yarn promote:to-prod",
    "ci:prepublish": "./scripts/prepublish.sh $STAGE",
    "ci:publish": "./scripts/publish.sh $STAGE",
    "docker:build": "docker build -f ../Dockerfile.chummy-builder -t alexkim205/ci:chummy-builder .",
    "docker:push": "yarn docker:build && docker push alexkim205/ci:chummy-builder"
  },
  "dependencies": {
    "@aws-amplify/api": "^3.2.16",
    "@aws-amplify/auth": "^3.4.16",
    "@aws-amplify/core": "^3.8.8",
    "@loadable/component": "^5.14.1",
    "@octokit/graphql": "^4.5.4",
    "@octokit/plugin-throttling": "^3.3.1",
    "@octokit/rest": "^18.0.6",
    "@primer/octicons-react": "^11.0.0",
    "chalk": "^4.1.0",
    "lodash.clonedeep": "^4.5.0",
    "mobx": "^5.15.6",
    "mobx-react-lite": "^2.2.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "quick-score": "^0.0.9",
    "react": "^16.13.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.9.5",
    "react-resizable": "^1.11.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "styled-components": "^5.2.0",
    "styled-theming": "^2.2.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.14.0",
    "@types/chrome": "^0.0.123",
    "@types/react": "^16.9.49",
    "assets-webpack-plugin": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-minify-constant-folding": "^0.5.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-tailcall-optimization": "^2.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bundlesize": "^0.18.0",
    "ci-info": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.2",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.1.0",
    "create-file-webpack": "^1.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "cypress": "6.1.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cz-conventional-changelog": "3.3.0",
    "detect-newline": "^3.1.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.5.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.1",
    "prettier": "^2.1.1",
    "serve": "^11.3.2",
    "sinon-chrome": "^3.0.1",
    "standard-version": "^9.0.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "weallcontribute": "^1.0.9",
    "webextension-polyfill": "^0.6.0",
    "webextension-polyfill-ts": "^0.21.0",
    "webpack": "^5.0.0-rc.6",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.2.0"
  },
  "lint-staged": {
    "**/*.+(js|jsx|json|css|md)": [
      "prettier --write"
    ],
    "**/*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "bundlesize": [
    {
      "path": "./dist/dev/[!popup_]*.js",
      "maxSize": "2 mB",
      "compression": "none"
    },
    {
      "path": "./dist/+(moz|web)/[!popup_]*.js",
      "maxSize": "300 kB",
      "compression": "none"
    }
  ]
}
