{
  "name": "Chummy",
  "description": "Website for Chummy Extension",
  "extensionId": "ocmdenamdoeigigibgjfnconlhpekfgb",
  "version": "1.0.0",
  "author": "Alex Kim",
  "email": "chummy@atomiccode.io",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexkim205/chummy"
  },
  "bugs": {
    "url": "https://github.com/alexkim205/chummy/issues"
  },
  "scripts": {
    "creds": "aws s3 cp s3://chummy-credentials/.env.development .env.development && aws s3 cp s3://chummy-credentials/key.pem key.pem",
    "mock:backend": "amplify mock",
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "format:check": "prettier '**/*.{js,jsx}' --check",
    "format": "prettier '**/*.{js,jsx}' --write",
    "lint:check": "eslint --ext .js,.jsx .",
    "lint": "eslint --fix --ext .js,.jsx .",
    "checkout:dev": "git checkout website/dev && git pull && amplify env checkout dev && amplify pull --yes",
    "checkout:gamma": "git checkout website/gamma && git pull && amplify env checkout gamma && amplify pull --yes",
    "checkout:prod": "git checkout website/prod && git pull && amplify env checkout prod && amplify pull --yes",
    "promote:to-dev": "./scripts/promote.dev.sh",
    "promote:to-gamma": "./scripts/promote.gamma.sh",
    "promote:to-prod": "./scripts/promote.prod.sh"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^0.2.33",
    "@loadable/component": "^5.14.1",
    "@primer/octicons-react": "^11.2.0",
    "@reach/router": "^1.3.4",
    "aws-amplify": "^3.3.13",
    "clsx": "^1.1.1",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.14.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "stripe": "^8.131.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "commitizen": "^4.2.2",
    "commitlint": "^11.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "gatsby-plugin-postcss": "^3.6.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "postcss": "^8.2.4",
    "prettier": "2.2.1",
    "standard-version": "^9.0.0",
    "tailwindcss": "^2.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "lint-staged": {
    "**/*.+(js|jsx|json|css|md)": [
      "prettier --write"
    ],
    "**/*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
